import java.util.Arrays;

class Solution {
    public int minMoves(int[] nums) {
        Arrays.sort(nums);
        int total = 0;
        int i = 0;
        int n = nums.length;
        while (i < n)
        {
            total += nums[n-1] - nums[i];
            i++;
        }

        return total;

    }
}