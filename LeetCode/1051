import java.util.Arrays;

class Solution {
    public int heightChecker(int[] heights) {
        int [] check = heights.clone();
        Arrays.sort(check);
        int count = 0;
        for (int i = 0; i < heights.length; i++)
        {
            if (check[i] != heights[i])
            {
                count++;
            }
        }
        return count;
    }
}